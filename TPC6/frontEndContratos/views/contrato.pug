extends layout

block content
  .w3-card-4
    header.w3-container.w3-blue 
      h1 Contrato - #{contrato._id}
    .w3-container
      table.w3-table-all

        tr
          td 
            Strong Anúncio
          td= contrato.nAnuncio || 'N/A'
        tr
          td 
            Strong Tipo de Procedimento
          td= contrato.tipoprocedimento
        tr
          td 
            Strong Objeto de Contrato
          td= contrato.objectoContrato
        tr
          td 
            Strong Data de Publicação
          td= contrato.dataPublicacao
        tr
          td 
            Strong Data de Celebração
          td= contrato.dataCelebracaoContrato
        tr
          td 
            Strong Preço Contratual
          td #{contrato.precoContratual}€
        tr
          td 
            Strong Praxo de Execução
          td= contrato.prazoExecucao
        tr
          td 
            Strong NIPC da Entidade Comunicante
          td= contrato.NIPC_entidade_comunicante
        tr
          td 
            Strong Entidade Comunicante
          td= contrato.entidade_comunicante
        tr
          td 
            Strong Fundamentação
          td= contrato.fundamentacao
        

    footer.w3-container.w3-blue
      adress Generated by EngWeb 2025 class - 
        a(href="/") Voltar à página principal